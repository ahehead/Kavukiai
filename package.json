{
  "displayName": "My Electron App",
  "name": "my-electron-app",
  "description": "Desktop app to play with local AI and other things in Node UI.",
  "version": "0.0.0",
  "main": "./node_modules/.dev/main/index.js",
  "resources": "src/resources",
  "author": {
    "name": "ahehead",
    "email": "nojoycrystal@gmail.com"
  },
  "license": "MIT",
  "packageManager": "pnpm@10.0.0",
  "pnpm": {
    "onlyBuiltDependenciesFile": "./trusted-dependencies-scripts.json"
  },
  "scripts": {
    "start": "electron-vite preview",
    "predev": "run-s clean:dev",
    "dev": "cross-env NODE_ENV=development electron-vite dev --watch",
    "compile:app": "cross-env-shell electron-vite build --config \"$INIT_CWD/electron.vite.config.ts\"",
    "compile:packageJSON": "tsx ./src/lib/electron-app/release/modules/prebuild.ts",
    "prebuild": "run-s clean:dev compile:app compile:packageJSON",
    "build": "pnpm electron-builder",
    "postinstall": "run-s prebuild install:deps",
    "install:deps": "electron-builder install-app-deps",
    "make:release": "tsx ./src/lib/electron-app/release/modules/release.ts",
    "release": "electron-builder --publish always",
    "clean:dev": "rimraf ./node_modules/.dev",
    "lint": "biome lint --no-errors-on-unmatched",
    "lint:fix": "biome lint --write --no-errors-on-unmatched",
    "prepare": "husky",
    "test": "vitest",
    "extract-types": "tsx scripts/extract-types.ts"
  },
  "dependencies": {
    "@lmstudio/sdk": "^1.3.0",
    "@radix-ui/react-context-menu": "^2.2.14",
    "@radix-ui/react-dropdown-menu": "^2.1.12",
    "@radix-ui/react-menubar": "^1.1.13",
    "@radix-ui/react-popover": "^1.1.11",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@saintno/comfyui-sdk": "^0.2.47",
    "@sinclair/typebox": "^0.34.33",
    "@sinclair/typebox-codegen": "^0.11.1",
    "@types/write-file-atomic": "^4.0.3",
    "@zubridge/electron": "^1.3.0",
    "arktype": "^2.1.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "electron-conf": "^1.3.0",
    "electron-router-dom": "^2.1.0",
    "fast-json-stable-stringify": "^2.1.0",
    "json-schema-to-typescript": "^15.0.4",
    "lucide-react": "^0.513.0",
    "openai": "^5.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.2.0",
    "remark-gfm": "^4.0.1",
    "rete": "^2.0.5",
    "rete-area-plugin": "^2.1.3",
    "rete-connection-plugin": "^2.0.5",
    "rete-context-menu-plugin": "^2.0.6",
    "rete-engine": "^2.1.1",
    "rete-history-plugin": "^2.1.0",
    "rete-react-plugin": "^2.0.7",
    "rete-render-utils": "^2.0.3",
    "sonner": "^2.0.3",
    "styled-components": "^6.1.18",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "write-file-atomic": "^6.0.0",
    "yaml": "^2.8.0",
    "zustand": "^5.0.3",
    "zustand-debounce": "^2.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.2",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.0.9",
    "@types/node": "^22.13.8",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/semver": "^7.5.8",
    "@vitejs/plugin-react": "^4.0.4",
    "code-inspector-plugin": "^0.20.1",
    "cross-env": "^7.0.3",
    "dts-bundle-generator": "^9.5.1",
    "electron": "^36.3.2",
    "electron-builder": "^26.0.12",
    "electron-extension-installer": "^1.2.0",
    "electron-vite": "^4.0.0",
    "fast-glob": "^3.3.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "npm-run-all": "^4.1.5",
    "open": "^10.1.0",
    "rimraf": "^6.0.1",
    "rollup-plugin-inject-process-env": "^1.3.1",
    "semver": "^7.5.4",
    "tailwindcss": "^4.0.9",
    "ts-morph": "^26.0.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "vite": "^7.0.5",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,css,json}": [
      "biome lint --write"
    ]
  }
}
